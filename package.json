{
    "name": "coc.js-sample-bot",
    "version": "0.0.2",
    "description": "Sample discord bot using clashofclans.js and discord.js",
    "repository": "https://github.com/r-priyam/cocjs-smaple-bot.git",
    "license": "MIT",
    "author": "r-priyam <roy_priyam@outlook.com>",
    "scripts": {
        "build": "rimraf dist && tsc",
        "format": "prettier --write .",
        "lint": "eslint src --ext ts --fix",
        "prepare": "husky install",
        "release": "standard-version",
        "release:preview": "standard-version --dry-run",
        "start": "npm run build && node --enable-source-maps dist/src/index.js",
        "sync:commands": "npm run build && node --enable-source-maps dist/src/sync-commands.js",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@prisma/client": "^3.15.2",
        "clashofclans.js": "^2.8.0",
        "dayjs": "^1.11.3",
        "discord.js": "^14.0.1",
        "dotenv": "^16.0.1",
        "picocolors": "^1.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@discordjs/rest": "^1.0.0",
        "@sapphire/eslint-config": "^4.3.7",
        "@types/node": "^17.0.21",
        "@types/prettier": "^2",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-xo": "^0.41.0",
        "eslint-config-xo-typescript": "^0.51.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unicorn": "^43.0.1",
        "husky": "^8.0.1",
        "joi": "^17.6.0",
        "prettier": "^2.7.1",
        "prisma": "^3.15.2",
        "rimraf": "^3.0.2",
        "standard-version": "^9.3.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "packageManager": "yarn@3.2.1"
}
