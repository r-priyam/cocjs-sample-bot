{
    "name": "cocjs-smaple-bot",
    "version": "1.0.0",
    "description": "Sample discord bot using clashofclans.js and discord.js",
    "license": "MIT",
    "author": "r-priyam <roy_priyam@outlook.com>",
    "scripts": {
        "build": "rimraf dist && tsc",
        "format": "prettier --write .",
        "lint": "eslint \"**/*.ts\"",
        "lint:fix": "eslint \"**/*.ts\" --fix",
        "start": "npm run build && node --enable-source-maps dist/src/index.js",
        "sync:commands": "npm run build && node --enable-source-maps dist/src/sync-commands.js",
        "typecheck": "tsc --noemit",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ogma/styler": "^1.0.0",
        "@prisma/client": "^3.11.1",
        "clashofclans.js": "^2.6.1",
        "dayjs": "^1.11.0",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.19.4",
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@discordjs/rest": "^0.3.0",
        "@types/node": "^17.0.21",
        "discord-api-types": "^0.31.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "joi": "^17.6.0",
        "prisma": "^3.11.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    }
}
