{
    "name": "cocjs-smaple-bot",
    "version": "0.0.1",
    "description": "Sample discord bot using clashofclans.js and discord.js",
    "repository": "https://github.com/r-priyam/cocjs-smaple-bot.git",
    "license": "MIT",
    "author": "r-priyam <roy_priyam@outlook.com>",
    "scripts": {
        "build": "rimraf dist && tsc",
        "format": "prettier --write .",
        "lint": "eslint \"**/*.ts\"",
        "lint:fix": "eslint \"**/*.ts\" --fix",
        "prepare": "husky install",
        "release": "standard-version",
        "release:preview": "standard-version --dry-run",
        "start": "npm run build && node --enable-source-maps dist/src/index.js",
        "sync:commands": "npm run build && node --enable-source-maps dist/src/sync-commands.js",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@prisma/client": "^3.12.0",
        "clashofclans.js": "^2.6.1",
        "dayjs": "^1.11.1",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "picocolors": "^1.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@discordjs/rest": "^0.4.1",
        "@types/node": "^17.0.21",
        "@types/prettier": "^2",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "discord-api-types": "^0.31.2",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-xo": "^0.40.0",
        "eslint-config-xo-typescript": "^0.50.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "joi": "^17.6.0",
        "prettier": "^2.6.2",
        "prisma": "^3.12.0",
        "rimraf": "^3.0.2",
        "standard-version": "^9.3.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "packageManager": "yarn@3.2.0"
}
